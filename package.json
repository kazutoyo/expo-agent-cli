{
  "name": "expo-agent-cli",
  "version": "0.1.3",
  "private": true,
  "description": "CLI tool for accessing Expo SDK documentation and search",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun run --filter '*' build",
    "dev": "bun run --filter '@expo-agent/cli' dev",
    "test": "bun test",
    "test:run": "bun test --run",
    "commitlint": "commitlint --edit",
    "release": "bun run --filter '@expo-agent/cli' release",
    "check": "biome check --write packages",
    "ci": "biome ci packages",
    "lint": "biome lint packages",
    "format": "biome format --write packages",
    "build-index": "bun run --filter '@expo-agent/indexer' build-index"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@evilmartians/lefthook": "^2.0.1",
    "@types/bun": "latest",
    "tsup": "^8.3.5",
    "typescript": "^5"
  },
  "packageManager": "bun@1.3.0",
  "author": "Kazutoyo Tokai <kazutoyo.jp@gmail.com> (https://github.com/kazutoyo)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazutoyo/expo-agent-cli.git"
  },
  "homepage": "https://github.com/kazutoyo/expo-agent-cli"
}
