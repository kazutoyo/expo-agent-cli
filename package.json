{
  "name": "expo-agent-cli",
  "version": "0.1.0",
  "description": "CLI tool for accessing Expo SDK documentation and search",
  "type": "module",
  "bin": {
    "expo-agent-cli": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "test:run": "bun test --run",
    "commitlint": "commitlint --edit",
    "release": "standard-version",
    "check": "biome check --write src",
    "ci": "biome ci src",
    "lint": "biome lint src",
    "format": "biome format --write src"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "algoliasearch": "^5.17.1",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@evilmartians/lefthook": "^2.0.1",
    "@types/bun": "latest",
    "standard-version": "^9.5.0",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "expo",
    "cli",
    "documentation",
    "agent",
    "sdk"
  ],
  "expoVersion": "sdk-54",
  "author": "Kazutoyo Tokai",
  "license": "MIT"
}
