{
	"name": "@expo-agent/cli",
	"version": "0.1.3",
	"description": "CLI tool for accessing Expo SDK documentation and search",
	"type": "module",
	"bin": {
		"expo-agent-cli": "./dist/cli.js"
	},
	"files": [
		"dist",
		"src/data"
	],
	"scripts": {
		"build": "tsup",
		"postbuild": "publint",
		"dev": "tsup --watch",
		"test": "bun test",
		"release": "np"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"dependencies": {
		"@expo-agent/core": "workspace:*",
		"@expo-agent/search": "workspace:*",
		"chalk": "^5.3.0",
		"commander": "^14.0.0"
	},
	"devDependencies": {
		"np": "^10.0.4",
		"publint": "^0.2.8"
	},
	"keywords": [
		"expo",
		"cli",
		"documentation",
		"agent",
		"sdk"
	],
	"expoVersion": "sdk-54",
	"author": "Kazutoyo Tokai <kazutoyo.jp@gmail.com> (https://github.com/kazutoyo)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kazutoyo/expo-agent-cli.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"homepage": "https://github.com/kazutoyo/expo-agent-cli"
}
